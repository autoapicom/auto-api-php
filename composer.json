{
    "name": "autoapi/client",
    "description": "PHP wrapper for the auto-api.com car listings API (encar, mobile.de, autoscout24 and more)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "auto-api",
        "api-client",
        "car-listings",
        "encar",
        "mobile-de",
        "autoscout24",
        "php-api-client",
        "car-auction",
        "scraper",
        "encar api",
        "mobile.de api",
        "autoscout24 api",
        "che168 api",
        "dongchedi api",
        "guazi api",
        "dubicars api",
        "dubizzle api"
    ],
    "homepage": "https://auto-api.com",
    "authors": [
        {
            "name": "AUTO-API.COM",
            "email": "access@auto-api.com"
        }
    ],
    "support": {
        "email": "access@auto-api.com",
        "docs": "https://auto-api.com"
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "AutoApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AutoApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
